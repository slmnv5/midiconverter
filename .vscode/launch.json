    "version": "0.2.0",
    "configurations": [
    
    {
        "name": "my_debug",
        "type": "cppdbg",
        "request": "launch",
        "program": "./mimap_d",
        "args": ["-c", "count.txt ", "-vvv"],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            }
        ]
    } 
    ]
}